<% extend('base') %>
<% if (isLogin) { %>
<a href='/blog/create'>Tạo bài viết</a>
<%} %>
<% if (posts[0]) { %>
<% posts.forEach(function(post){ %>
    <h3 ><a href="/blog/<%= post.slug %>"><%= post.title %></a></h3>
    <h6><%= post.date %></h6>
<% }); %>
<% } else { %>
    <h5>Không có bài viết nào</h5>
<% }%>